{
    "name": "usermanagement",
    "version": "0.0.0",
    "main": "app.js",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run db:migrate:dev && && npm run db:seed:dev\" \"babel-node ./src/app.js\"",
        "dev": "nodemon --exec babel-node ./src/app.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "db:migrate:dev": "./node_modules/.bin/sequelize db:migrate",
        "db:seed": "sequelize db:seed:all",
        "db:seed:dev": "./node_modules/.bin/sequelize db:seed:all",
        "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all"
    },
    "dependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/node": "^7.12.6",
        "@babel/preset-env": "^7.12.7",
        "@sendgrid/mail": "^7.4.0",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.23.0",
        "concurrently": "^5.3.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "helmet": "^4.2.0",
        "http-errors": "^1.8.0",
        "joi": "^17.3.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "morgan": "~1.9.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.2.5",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.3.5",
        "sequelize-cli": "^6.2.0"
    },
    "devDependencies": {
        "eslint": "^7.14.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.6"
    }
}
